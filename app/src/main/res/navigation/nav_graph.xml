<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/placeholder">


    <fragment android:id="@+id/placeholder"
        tools:layout="@layout/activity_home">
        <action
            android:id="@+id/action_placeholder_to_onBoardingFragment"
            app:destination="@id/onBoardingFragment"
            />
    </fragment>

    <fragment
        android:id="@+id/onBoardingFragment"
        android:name="com.dhruvdroid.jetpack.jetpack.OnBoardingFragment"
        android:label="OnBoardingFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_onBoardingFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/userProfileFragment"
        android:name="com.dhruvdroid.jetpack.fragment.UserProfileFragment"
        android:label="UserProfileFragment" />
</navigation>